#!/usr/bin/env bash
# This script displays "Best School" 10 times, with "Hi" on the 9th iteration

# Initialize a counter
counter=0

# Use a while loop to display "Best School" 10 times
while [ $counter -lt 10 ]; do
    if [ $counter -eq 9 ]; then
        echo "Hi"
    else
        echo "Best School"
    fi
    ((counter++))
done
